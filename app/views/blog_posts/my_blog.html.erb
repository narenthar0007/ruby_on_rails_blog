<table style="background-color:rgba(0, 1, 0, 0.44);color:beige; width:1400px">
                <th style="background-color:rgba(246, 19, 19, 0.44)">
                        <h5>My Blog page</h5>
                </th>
                <th>
                    <i class="fa fa-arrow-left"></i><%= link_to "back", :back %> 
    
                </th>
                <th>
                        <h5><%=link_to "Home", root_path%></h5>
                </th>
                
                <th>
                        <h5><%= link_to "New Blog",new_blog_post_path %></h5>
                </th>
                <th>
                        <h5><%= link_to "Profile ", edit_user_registration_path %></h5>
                </th>
                <th>
                        <%if !(user_signed_in?) %>
                        <h5><%= link_to "Log In", new_user_session_path%></h5>
                        <%end%>
                </th>
                <th>
                        <h5><%= link_to "All Blogs", blog_post_home_path%></h5>
                </th>
                <th>
                        <%if (user_signed_in?) %>
                        <h5><%= link_to "Liked Blog",liked_path %></h5>
                        <%end%>

                </th>
                <th>
                        <h5><%= link_to "Trending Blogs", treanding_blog_path%></h5>
                </th>
                <th>
                        <h5><%= button_to "log out " , destroy_user_session_path ,method: :delete, data: {turbo_confirm: "are you sure"}%></h5>
                </th>
                <th>
                        <h5> <form class="form-inline my-2 my-lg-0" action="/search" >
                                <input class="form-control mr-sm-2" name="q" type="search" placeholder="Search" method="get" aria-label="Search" >
                                <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search Blog</button>
                        </form></h5>
                </th>
        </table>

<div>
  <div>
    <img  class = "i"src="https://upload.wikimedia.org/wikipedia/commons/thumb/9/92/Open_book_nae_02.svg/2560px-Open_book_nae_02.svg.png" alt="">
 <%bc=@blog_post.count%>   
<%if bc == 0 %>             
    <h3>You have not created a blog yet start your bloging now.....!</h3>
    <%= image_tag "https://www.bing.com/th/id/OGC.79128dc5a6551edc602a8db095084fe2?pid=1.7&rurl=https%3a%2f%2fi.pinimg.com%2foriginals%2faf%2f55%2f87%2faf5587ad1fa5ca160048939e6cae5fa2.gif&ehk=oXm4JT2VHb5rL%2bXLSeck4orcyFX0gzrHWM25Moecf3M%3d" %>
    <h5><%= link_to "New Blog",new_blog_post_path %></h5>

<%else%>
    <h1>My Blog count : <%= %></h1>    
    <% @blog_post.each do |b| %>
    <h1 class="display-4">Title :<%= link_to b.title, blog_post_path(b.id)%></h1>
    <h5>Blog Body:<%=b.body%></h5>
    <p class="b_secondaryText callout_vis">Blog ID : <%=b.id%></p>
<p>user id: <%=b.user_id%></p>
<%end%>
<%end%>  
<h5><%= link_to "Liked Blog",liked_path %></h5>
